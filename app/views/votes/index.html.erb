<p id="notice"><%= notice %></p>

<h1>Votes</h1>

<table>
  <thead>
    <tr>
      <th>Candidate</th>
      <th>Primary quality</th>
      <th>Secondary quality</th>
      <th>Tertiary quality</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.candidate_id %></td>
        <td><%= vote.primary_quality_id %></td>
        <td><%= vote.secondary_quality_id %></td>
        <td><%= vote.tertiary_quality_id %></td>
        <td><%= vote.comment %></td>
        <td><%= link_to 'Show', vote %></td>
        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_vote_path %>
